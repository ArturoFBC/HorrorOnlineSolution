@model IEnumerable<StoryResponse>
@{
	ViewData["Title"] = "Resultados de la búsqueda";
}

@if (Model == null || Model.Count() == 0)
{
	<span class="text-message justify-self-center">No se encontraron relatos.</span>
}
else
{
	@foreach (StoryResponse story in Model)
	{
		<partial name="PartialViews/StoryPartialView" model="story" />
	}
}